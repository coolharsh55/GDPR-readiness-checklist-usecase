        <rdfs:isDefinedBy rdf:resource="http://purl.org/adaptcentre/openscience/resources/GDPRtEXT#recital50"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Purpose of new processing</rdfs:label>
    </owl:Class>
    


