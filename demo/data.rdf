        <rdfs:isDefinedBy rdf:resource="http://purl.org/adaptcentre/openscience/resources/GDPRtEXT#recital18"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Personal or Household activity</rdfs:label>
    </owl:Class>
    


